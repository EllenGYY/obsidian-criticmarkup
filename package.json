{
  "name": "obsidian-criticmarkup",
  "version": "0.1.4",
  "description": "A CriticMarkup plugin for Obsidian",
  "main": "main.js",
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
    "build:dev": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs development",
    "lint": "npx eslint src/",
    "format": "npx prettier --write src/",
    "test": "jest"
  },
  "keywords": [],
  "author": "kometenstaub and Fevol",
  "license": "MIT",
  "devDependencies": {
    "@codemirror/language": "git://github.com/lishid/cm-language.git#main",
    "@flatten-js/interval-tree": "^1.1.2",
    "@lezer/generator": "^1.6.0",
    "@lezer/lr": "^1.4.0",
    "@tsconfig/svelte": "^5.0.2",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.22",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "builtin-modules": "^3.3.0",
    "bun-types": "^1.0.29",
    "esbuild": "^0.20.1",
    "esbuild-jest": "^0.5.0",
    "esbuild-plugin-inline-worker": "https://github.com/mitschabaude/esbuild-plugin-inline-worker",
    "esbuild-sass-plugin": "^3.1.0",
    "esbuild-svelte": "^0.8.0",
    "eslint": "^8.57.0",
    "eslint-plugin-svelte": "^2.35.1",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-expect-message": "^1.1.3",
    "jsdom": "^24.0.0",
    "localforage": "^1.10.0",
    "localforage-getitems": "https://github.com/conversejs/localForage-getItems/tree/master",
    "localforage-setitems": "^1.4.0",
    "monkey-around": "^3.0.0",
    "obsidian": "latest",
    "prettier": "3.2.5",
    "process": "^0.11.10",
    "svelte": "^4.2.12",
    "svelte-eslint-parser": "^0.33.1",
    "svelte-preprocess": "^5.1.3",
    "ts-jest": "^29.1.2",
    "tslib": "^2.6.2",
