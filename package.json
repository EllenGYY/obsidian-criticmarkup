{
    "name": "obsidian-criticmarkup",
    "version": "0.2.1",
    "type": "module",
    "description": "A CriticMarkup plugin for Obsidian",
    "main": "main.js",
    "scripts": {
        "dev": "bun run esbuild.config.mjs",
        "build": "bun run tsc -noEmit -skipLibCheck && bun scripts/build/esbuild.config.ts production",
        "build:dev": "bun run tsc -noEmit -skipLibCheck && bun scripts/build/esbuild.config.ts development",
        "lint": "bun eslint src/",
        "format": "bun dprint fmt",
        "test": "jest --verbose",
        "release-patch": "bun run scripts/release/bump-version.ts --type patch",
        "release-minor": "bun run scripts/release/bump-version.ts --type minor",
        "release-major": "bun run scripts/release/bump-version.ts --type major",
        "release-rc": "bun run scripts/release/bump-version.ts --rc"
    },
    "keywords": [],
    "author": "kometenstaub and Fevol",
    "license": "MIT",
    "devDependencies": {
        "@types/jest": "^29.5.14",
        "@typescript-eslint/eslint-plugin": "^8.32.0",
        "@typescript-eslint/parser": "^8.32.0",
        "commander": "^13.1.0",
        "dprint": "^0.49.1",
        "eslint": "^9.26.0",
        "eslint-plugin-deprecation": "^3.0.0",
        "eslint-plugin-svelte": "^3.5.1",
        "jest": "^29.7.0",
        "jest-environment-jsdom": "^29.7.0",
        "jest-expect-message": "^1.1.3",
        "jsdom": "^26.1.0",
        "prettier": "3.5.3",
        "svelte-eslint-parser": "^1.1.3",
        "ts-jest": "^29.3.2",
        "tslib": "^2.8.1"
    },
    "dependencies": {
        "@codemirror/buildhelper": "^1.0.2",
        "@codemirror/language": "github:lishid/cm-language",
        "@flatten-js/interval-tree": "^1.1.3",
        "@lezer/generator": "^1.7.3",
        "@lezer/lr": "^1.4.2",
        "@tsconfig/svelte": "^5.0.4",
        "@types/bun": "^1.2.12",
        "@types/diff-match-patch": "^1.0.36",
        "@types/node": "^22.15.17",
        "@types/obsidian-typings": "npm:obsidian-typings@^3.7.0",
        "builtin-modules": "^5.0.0",
        "diff-match-patch": "^1.0.5",
        "esbuild": "^0.25.4",
        "esbuild-jest": "^0.5.0",
        "esbuild-plugin-inline-worker": "https://github.com/mitschabaude/esbuild-plugin-inline-worker",
        "esbuild-sass-plugin": "^3.3.1",
        "esbuild-svelte": "^0.9.2",
        "localforage": "^1.10.0",
        "localforage-getitems": "github:conversejs/localForage-getItems",
        "localforage-setitems": "^1.4.0",
        "monkey-around": "^3.0.0",
        "obsidian": "^1.8.7",
        "obsidian-typings": "3.7.0",
        "svelte": "^5.28.2",
        "svelte-preprocess": "^6.0.3",
        "svelty-picker": "^6.1.5",
        "typescript": "^5.8.3"
    },
    "trustedDependencies": [
        "@codemirror/language"
    ]
}